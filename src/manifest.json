{
	"name": "Web Maker",
	"version": "1.2",
	"manifest_version": 2,
	"description": "Make every new tab an offline playground for your web experiments",
	"chrome_url_overrides" : {
		"newtab": "index.html"
	},
	"homepage_url": "http://kushagragour.in/lab/web-maker",
	"permissions": [
		"storage"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-oEJh/gQdHXu0Vw34Jd7uSMA+d9M2HnPWjJvHIqRgs0w='; object-src 'self'",
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png"
	}
}
